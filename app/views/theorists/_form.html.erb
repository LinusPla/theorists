<%= form_with model: theorist do |f| %>
  <%= f.label :stage_name %>
  <%= f.text_field :stage_name %>
  <%= f.label :main_theory %>
  <%= f.text_field :main_theory %>
  <%= f.label :sources %>
  <%= f.text_field :sources %>
  <%= f.label :price %>
  <%= f.text_field :price %>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= f.label :rating %>
  <%= f.number_field :rating %>
  <%= f.submit %>
<% end %>
